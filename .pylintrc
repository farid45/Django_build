[MASTER]
load-plugins=pylint_django
ignore=migrations
init-hook=
    import os; os.environ['DJANGO_SETTINGS_MODULE'] = 'pylint_settings'

[MESSAGES CONTROL]
disable=
    missing-docstring,
    too-few-public-methods,
    invalid-name,
    import-error,
    unused-argument,
    wrong-import-position

[FORMAT]
max-line-length=120
